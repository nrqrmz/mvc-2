<h1>Contact</h1>
<br>
<ul>
  <% @members.each do |member| %>
    <li>
      <%= member.capitalize %>
    </li>
  <% end %>
</ul>
<br>
<form action="/contact" method="get">
  <input
    type="text"
    name="member"
    placeholder="Who are you looking for?"
    value="<%= params[:member] %>"
  >
  <input type="submit">
</form>
<br>
<%= link_to 'Back home', root_path %>
